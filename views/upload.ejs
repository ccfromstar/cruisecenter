<html>
<head>
<script src="http://libs.baidu.com/jquery/2.0.0/jquery.min.js"></script>
<link href="http://libs.baidu.com/bootstrap/3.0.3/css/bootstrap.min.css" rel="stylesheet">
<script src="http://libs.baidu.com/bootstrap/3.0.3/js/bootstrap.min.js"></script>
<link href="/css/layout.css" rel="stylesheet" type="text/css" />
</head>
<body>
<form method="post" enctype="multipart/form-data" action="uploaddo">
	<table>
		<tr>
			<td><input type="file" multiple="multiple" name="img_url" ></td>
			<td><button type="button" class="btn btn-default" onclick="_newForm()">导入</button></td>
		</tr>
	</table>
	<input type="hidden" id="line_info_list" name="line_info_list" >
</form>
<script>
    function _newForm(){
        var line_info_list = window.parent.document.all.line_info_list.value;
        if(line_info_list == ""){
        	alert("请选择航次日期");return false;
        }
    	if(document.all.img_url.value == ""){
    		alert("请选择您要上传的文件");return false;
    	}
    	$("#line_info_list").val(line_info_list);
    	document.forms[0].submit();
    }
</script>
</body>
</html>